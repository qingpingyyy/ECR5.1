			  dataList="{$IncomeAndExpenseStatementSgmt.FinancialSubsidyRevenue}-{$IncomeAndExpenseStatementSgmt.UndertakingsExpenditureFinancialSubsidyExpenditure}-{$IncomeAndExpenseStatementSgmt.UndertakingsExpenditureNonFinancialSubsidyExpenditure}"
