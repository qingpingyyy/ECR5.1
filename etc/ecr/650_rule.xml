			  dataList="10;20;30;" errorCode="ABE001" errorMsg="对于枚举型数据项，取值需要在代码表中。@“企业身份标识类型”取值需要在代码表中。"/>
		<rule name="I0000201-2" sgmtName="IncomeAndExpenseStatementBsSgmt" fieldName="SheetType" level="3" type="DIC"
			  dataList="SheetType" errorCode="ABE001" errorMsg="对于枚举型数据项，取值需要在代码表中。@“报表类型”取值需要在代码表中。"/>
		<rule name="I0000201-3" sgmtName="IncomeAndExpenseStatementBsSgmt" fieldName="SheetTypeDivide" level="3" type="DIC"
			  dataList="SheetTypeDivide" errorCode="ABE001" errorMsg="对于枚举型数据项，取值需要在代码表中。@“报表类型细分”取值需要在代码表中。"/>
		<rule name="I0000201-4" sgmtName="IncomeAndExpenseStatementBsSgmt" fieldName="RptDateCode" level="3" type="DIC"
			  dataList="RptDateCode610" errorCode="ABE001" errorMsg="对于枚举型数据项，取值需要在代码表中。@“报告时点说明代码”取值需要在代码表中。"/>

		<rule name="I6500B01" sgmtName="IncomeAndExpenseStatementBsSgmt" fieldName="SheetType" level="3"
			  type="DE" dataList="10" errorCode="CRE300"
			  errorMsg="“报表类型”必须填写“10”。">
		</rule>
		<rule name="I6500C01" sgmtName="IncomeAndExpenseStatementSgmt"
			  fieldName="CurrentFinancialSubsidyCarriedOverBalance" level="3"
