			  errorMsg="在事业单位收入支出表信息中数据项“本期财政补助结转结余=财政补助收入-事业支出”。">
		</rule>
		<rule name="I6500C02" sgmtName="IncomeAndExpenseStatementSgmt" fieldName="CurrentUndertakingsCarriedOverBalance"
			  level="3"
			  type="DE" compareWay="number"
			  dataList="{$IncomeAndExpenseStatementSgmt.UndertakingsClassRevenue}-{$IncomeAndExpenseStatementSgmt.UndertakingsClassExpenditure}"
			  errorCode="CRE302"
			  errorMsg="在事业单位收入支出表信息中数据项“本期事业结转结余=事业类收入-事业类支出”。">
		</rule>
		<rule name="I6500C03" sgmtName="IncomeAndExpenseStatementSgmt" fieldName="CurrentOperatingBalance" level="3"
			  type="DE" compareWay="number"
			  dataList="{$IncomeAndExpenseStatementSgmt.OperatingRevenue}-{$IncomeAndExpenseStatementSgmt.OperatingExpenditure}"
			  errorCode="CRE303"
			  errorMsg="在事业单位收入支出表信息中数据项“本期经营结余=经营收入-经营支出”。">
		</rule>
