<?xml version="1.0" encoding="GBK" ?>
<validator>
	<code>
		<rule name="I0000201-1" sgmtName="BalanceSheetBsSgmt" fieldName="EntCertType" level="3" type="DL"
			  dataList="10;20;30;" errorCode="ABE001" errorMsg="对于枚举型数据项，取值需要在代码表中。@“企业身份标识类型”取值需要在代码表中。"/>
		<rule name="I0000201-2" sgmtName="BalanceSheetBsSgmt" fieldName="SheetType" level="3" type="DIC"
			  dataList="SheetType" errorCode="ABE001" errorMsg="对于枚举型数据项，取值需要在代码表中。@“报表类型”取值需要在代码表中。"/>
		<rule name="I0000201-3" sgmtName="BalanceSheetBsSgmt" fieldName="SheetTypeDivide" level="3" type="DIC"
			  dataList="SheetTypeDivide" errorCode="ABE001" errorMsg="对于枚举型数据项，取值需要在代码表中。@“报表类型细分”取值需要在代码表中。"/>
		<rule name="I0000201-4" sgmtName="BalanceSheetBsSgmt" fieldName="RptDateCode" level="3" type="DIC"
			  dataList="RptDateCode610" errorCode="ABE001" errorMsg="对于枚举型数据项，取值需要在代码表中。@“报告时点说明代码”取值需要在代码表中。"/>

		<rule name="I6100C01" sgmtName="BalanceSheet2002Sgmt" fieldName="TotalEquityAndLiabilities" level="3"
			  type="DE" compareWay="number"
			  dataList="{$BalanceSheet2002Sgmt.TotalLiabilities}+{$BalanceSheet2002Sgmt.MinorityInterest}+{$BalanceSheet2002Sgmt.TotalEquity}"
			  errorCode="CRE000"
			  errorMsg="在借款人2002版资产负债表信息中数据项“负债合计＋少数股东权益＋所有者权益合计＝负债和所有者权益总计”。">
		</rule>
		<rule name="I6100C02" sgmtName="BalanceSheet2002Sgmt" fieldName="TotalEquityAndLiabilities" level="3"
			  type="DE" compareWay="number" dataList="{$BalanceSheet2002Sgmt.TotalAssets}" errorCode="CRE001"
			  errorMsg="在借款人2002版资产负债表信息中数据项“负债和所有者权益总计＝资产总计”。">
		</rule>
		<rule name="I6100D01" sgmtName="BalanceSheet2007Sgmt" fieldName="TotalEquityAndLiabilities" level="3"
			  type="DE" compareWay="number"
			  dataList="{$BalanceSheet2007Sgmt.TotalLiabilities}+{$BalanceSheet2007Sgmt.TotalEquity}" errorCode="CRE002"
			  errorMsg="在2007资产负债表信息中数据项“负债和所有者权益合计=负债合计+所有者权益合计”。">
		</rule>
		<rule name="I6100C02" sgmtName="BalanceSheet2007Sgmt" fieldName="TotalAssets" level="3"
			  type="DE" compareWay="number" dataList="{$BalanceSheet2007Sgmt.TotalEquityAndLiabilities}" errorCode="CRE001"
			  errorMsg="在2007版资产负债表信息中数据项“资产总计=负债和所有者权益合计”。">
		</rule>
	</code>
</validator>
